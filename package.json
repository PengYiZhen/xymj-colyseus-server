{
  "private": true,
  "name": "xymj-colyseus-server",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 20.9.0"
  },
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "watch-controllers": "node scripts/watch-controllers.mjs",
    "generate-controllers": "node scripts/watch-controllers.mjs",
    "loadtest": "tsx loadtest/example.ts --room my_room --numClients 2",
    "build": "npm run clean && npm run generate-controllers-once && tsc",
    "generate-controllers-once": "node scripts/generate-controllers.mjs",
    "clean": "rimraf dist",
    "test": "mocha -r tsx test/**_test.ts --exit --timeout 15000"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.16.0",
    "@colyseus/testing": "^0.16.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.10.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.10.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.16.0",
    "@colyseus/playground": "^0.16.0",
    "@colyseus/tools": "^0.16.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "colyseus": "^0.16.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.16.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.11.3",
    "routing-controllers-openapi": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.17"
  }
}
